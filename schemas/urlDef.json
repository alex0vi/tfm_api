{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "url": {
            "type": "string",
            "format": "url"
        },
        "id": {"type":"number"},
        "tel": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "dialCode":{
                            "type": "string",
                            "pattern": "(^\\+[\\w]{2})$"
                        }
                    }
                },
                { "type": "null" }
            ]
        }
    },
    "additionalProperties": false
}
